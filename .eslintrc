{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["react", "jsx-a11y", "import", "react-native"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb", "plugin:react-native/all"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "no-return-assign": 0
  },
  "globals": {
    "fetch": true,
    "__DEV__": true,
    "alert": true
  }
}
